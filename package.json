{
  "name": "homeschool-summer-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^6.16.0",
    "axios": "^1.13.2",
    "glob": "^11.0.0",
    "lru-cache": "^11.0.1",
    "react": "^19.2.3",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^6.0.1",
    "vite": "^5.1.6"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "overrides": {
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.18.0",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.4",
    "glob": "^11.0.0",
    "inflight": "npm:lru-cache@^11.0.1",
    "rimraf": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react-refresh/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "es2020": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
