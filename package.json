{
  "name": "homeschool-summer-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^6.16.0",
    "axios": "^1.13.2",
    "glob": "^11.0.0",
    "lru-cache": "^11.0.1",
    "react": "^19.2.3",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rimraf": "^6.0.1",
    "vite": "^5.1.6"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 10"
  },
  "overrides": {
    "glob": "^11.0.0",
    "inflight": "npm:lru-cache@^11.0.1",
    "rimraf": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "react",
      "react-hooks",
      "react-refresh"
    ],
    "rules": {
      "react-refresh/only-export-components": "warn",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect",
        "runtime": "automatic"
      }
    },
    "env": {
      "browser": true,
      "es2020": true,
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "ignorePatterns": [
      "build/**/*",
      "dist/**/*",
      "node_modules/**/*",
      "*.min.js",
      "**/*.chunk.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
